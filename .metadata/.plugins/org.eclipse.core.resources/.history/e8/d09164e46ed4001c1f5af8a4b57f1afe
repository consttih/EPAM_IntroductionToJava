package by.online.ht02.main;

public class Task_19 {

	public static void main(String[] args) {
		double a;
		double b;
		double c;
		double i;
		
		a = -1;
		b = 5;
		c = 6;
		i = 0;
		
		if (a > 0) {
			i = i + 1;
		};
		if (b > 0) {
			i = i + 1;
		};
		if (c > 0) {
			i = i + 1;
		};
		
		System.out.println(i);

	}

}
