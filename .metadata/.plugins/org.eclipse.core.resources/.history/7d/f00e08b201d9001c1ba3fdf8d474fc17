package by.jonline.ht03.main;

import java.util.Scanner;

public class Task_26 {

	public static void main(String[] args) {
		
		int n;
		char s;
		
		Scanner n1 = new Scanner(System.in);
		n = n1.nextInt();
		
		do {
			Scanner n2 = new Scanner(System.in);
			n = n2.nextInt();
			s = (char)(n);
			System.out.println(s);
		}
		while (n != 0);
	}

}
