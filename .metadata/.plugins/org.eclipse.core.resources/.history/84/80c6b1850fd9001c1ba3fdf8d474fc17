package by.jonline.ht03.main;

public class Task_27 {

	public static void main(String[] args) {
		
		int n, m;
		int s;
		m = 1;
		n = 10;
		
		for (int i = m; i <= n; i++) {
			if (i == 1) {
				System.out.println("For 1:\n>> 1");
			}else {
				
				System.out.println("For " + i + ":");
				
				s = 2;
				
				while (i != s) {
					
					if (i % s == 0) {
						System.out.println(">> " + s);
					};
					s++;
		
				}
			}
		}

	}

}
