package by.jonline.ht05.main;

public class Task_04 {

	public static void main(String[] args) {

		int n = 5;
		int m = 5;
		int a[][] = new int[m][n];

		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {

				if (i % 2 == 0) {
					for (int q = 1; q <= n; q++) {
						a[i][j] = q;
						System.out.print(a[i][j] + " ");
					};
				} else {
					for (int q = 0; q < n; q++) {
						a[i][j] = n - q;
						System.out.print(a[i][j] + " ");
					};

				}
				;

			}
			;
			System.out.println();
		}
		;

		System.out.println();

	}

}
