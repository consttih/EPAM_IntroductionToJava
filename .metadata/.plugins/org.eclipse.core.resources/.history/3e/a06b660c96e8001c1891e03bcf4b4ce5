package by.jonline.ht05.main;

public class Task_14 {

	public static void main(String[] args) {

		int n = 6;
		int m = 5;
		int p = 0;

		int a[][] = new int[n][m];

		for (int i = 0; i < n; i++) {
			for (int j = 0; j < m; j++) {
				do {
					a[i][j] = ((int) (Math.random() * 2));

					if (a[i][j] == 1) {
						p += 1;
					}
					;

					System.out.print(a[i][j] + " ");
				} while (p != i);
				System.out.println();

			}
			;
			System.out.println();

		}
		;
	}
}
