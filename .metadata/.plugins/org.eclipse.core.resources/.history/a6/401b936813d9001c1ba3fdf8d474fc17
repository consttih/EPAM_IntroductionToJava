package by.jonline.ht03.main;

public class Task_27 {

	public static void main(String[] args) {
		
		int n, m;
		int s;
		int q;
		m = 126;
		n = 138;
		q = 0;
		
		for (int i = m; i <= n; i++) {
			
				System.out.println("For " + i + ":");
				
				s = 2;
				
				while (i != s) {
					
				for (int p = 2; p < s; p++) {
					
					if (i % s == 0) {
						System.out.println(">> " + s);
						q += 1;
					};
				};
				if (q == 0) {
					System.out.println("the number has no divisors");
				};
				s++;
		
			};
		};
	}

}

