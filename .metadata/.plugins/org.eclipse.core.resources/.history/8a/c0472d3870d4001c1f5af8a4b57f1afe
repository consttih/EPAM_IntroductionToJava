package by.online.ht02.main;

public class Task_20 {

	public static void main(String[] args) {
		
		double a;
		double b;
		double c;
		double k;
		
		a = 52;
		b = 27;
		c = 9;
		k = 3;
		
		if (a % k == 0) {
			System.out.println(a);
		};
		if (a % k == 0) {
			System.out.println(a);
		}
		if (a % k == 0) {
			System.out.println(a);
		}

	}

}
