package by.online.ht02.main;

public class Task_28 {

	public static void main(String[] args) {
		double a, a2;
		double b, b2;
		double c, c2;
		double max;
		double d;
		
		a = 5;
		b = 6;
		c = 4;
		d = 8;
		a2 = d - a;
		b2 = d - b;
		c2 = d - c;
		
		if (a == d) {
			System.out.println("a = d = " + d);
		}if (b == d) {
			System.out.println("b = d = " + d);
		}if (c == d) {
			System.out.println("c = d = " + d);
		}else {
			if (a > b && a > c) {
				max = a;
			}if (b > a && b > c) {
				max = b;
			}else {
				max = c;
			}
			System.out.println(max);
			
		}

	}

}
