package by.jonline.ht05.main;

public class Task_01 {

	public static void main(String[] args) {

		int n = 6;
		int m = 5;
		double a[][] = new double[m][n];

		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {
				a[i][j] = ((double) (Math.random() * 51) - 25);
				System.out.print(a[i][j] + " ");
			}
			;
			System.out.println();
		}
		;
		
	}

}
