package by.jonline.ht05.main;

public class Task_05 {

	public static void main(String[] args) {

		int n = 5;
		int a[][] = new int[n][n];

		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n - i; j++) {
				a[i][j] = i + 1;
				System.out.print(a[i][j] + " ");
			for (int k = 0; k < i - 1; k++) {
				a[i][j] = i + 1;
				System.out.print(a[i][j] + " ");
				}
				;
			}
			;
			System.out.println();

		}
	}

}
