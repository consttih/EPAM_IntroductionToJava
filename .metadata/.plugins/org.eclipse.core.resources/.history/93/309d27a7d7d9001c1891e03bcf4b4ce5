package by.jonline.ht03.main;

public class Task_29 {

	public static void main(String[] args) {
		
		int a;
		int b;
		int c, d;
		int i, m;
		
		a = 123456;
		b = 456789;
		c = a;
		d = b;
		i = 0;
		m = 0;
		
		while (c > 0) {
			c /= 10;
			i += 1;
		};
		while (d > 0) {
			d /= 10;
			m += 1;
		};
		
		int[] a1 = new int[i];
		int[] b1 = new int[m];
		
		for (int e = 0; e < i; e++) {
			a1[e] = a % 10;
			a /= 10;
			System.out.println(a1[e]);
		}
		
		
	}

}
